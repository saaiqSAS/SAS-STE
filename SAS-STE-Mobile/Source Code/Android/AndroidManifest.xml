<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sas.ste.mobile">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/sas_ste_logo"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/sas_ste_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SAS_STE_Mobile"
        tools:targetApi="31">
        <activity
            android:name=".ViewStaticKeys"
            android:exported="false" />
        <activity
            android:name=".StaticKeyGen"
            android:exported="false" />
        <activity
            android:name=".OutputZoom"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".KeyGen"
            android:exported="false" />
        <activity
            android:name=".Input"
            android:exported="false" />
        <activity
            android:name=".KeyInput"
            android:exported="false" />
        <activity
            android:name=".Output"
            android:exported="false" />
    </application>

</manifest>